{

	"manifest_version":2,
	"version":"1.0",
	"name":"Calculator",
	"description":"This is a handy Calculator that will provide you with the basic operations in your browser",
	"icons":{
		"128":"icon128.png",
		"48":"icon48.png",
		"16":"icon16.png"
	},

	"browser_action":{
		"default_icon":"icon16.png",
		"default_popup":"popup.html"
	},

	"permissions": [
          "storage",
          "notifications"
	]


}